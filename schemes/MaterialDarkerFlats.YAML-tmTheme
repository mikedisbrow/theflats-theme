# [PackageDev] target_format: plist, ext: tmTheme
---
name: Material-Darker
uuid: 
author: Mike Disbrow (mikedisbrow.github.io)

colorSpaceName: sRGB

semanticClass: Material-Darker.dark
settings:
- settings:
    background: '#282828'
    caret: '#FFCC00'
    foreground: '#e0e0e0'
    invisibles: '#424242'
    lineHighlight: '#00000030'
    selection: '#61616140'
    guide: '#42424260'
    activeGuide: '#6E6E6E'
    stackGuide: '#42424280'
    gutterForeground: '#757575'
    findHighlight: '#F8E71C'
