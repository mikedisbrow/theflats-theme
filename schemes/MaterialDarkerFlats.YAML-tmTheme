    tagOptions: 'underline'

- name: Comments
  scope: comment, punctuation.definition.comment, text.cancelled, meta.punctuation.separator, markup.strikethrough
  settings:
    foreground: '#757575'
    fontStyle: 'italic'

- name: Text
