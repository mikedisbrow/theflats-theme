# [PackageDev] target_format: plist, ext: tmTheme
---
name: PreDawn Flats
uuid:
author: Mike Disbrow (mikedisbrow.github.io)

colorSpaceName: sRGB

semanticClass: predawn.flats.dark
settings:
- settings:
    background: '#282828'
    caret: '#F18260'
    foreground: '#e0e0e0'
    invisibles: '#F18260'
    lineHighlight: '#232323'
    selection: '#61616140'
    #inactiveSelection: '#3C3C3C'
    findHighlight: '#F18260'
    gutterForeground: '#757575'
    guide: '#42424260'
    stackGuide: '#42424280'
    activeGuide: '#F1826050'
    minimapBorder: '#F1826025'
    shadow: '#00000010'
    tagOptions: 'underline'
    popupCSS: 'html {
            background-color: #2d2d2d;
            color: #e0e0e0;
          }
          a {
            color: #bddcdc;
          }
          .error, .deleted {
            color: #CF5340;
          }
          .success, .inserted {
            color: #B4D388;
          }
          .warning, .modified {
            color: #92BFBF;
          }'

- name: Comments
  scope: comment
  settings:
    foreground: '#757575'

- name: Python Docstring
  scope: string.quoted.double.block.python, text.html.django storage.type.templatetag.django storage.type.attr.django, text.html.jekyll punctuation.tag.jekyll string.quoted.single.jekyll, text.html.swig meta.function.block.start.swig string.quoted.single.html
  settings: 
    foreground: '#757575'

- name: Constant
  scope: constant
  settings: 
    foreground: '#EDE480'

- name: Entity
  scope: entity
  settings: 
    foreground: '#92BFBF'

- name: Javascript - White
  scope: source.js, variable.other.class.js, variable.other.property.static.js, entity.name.function.js, keyword.operator.accessor.js, variable.other.readwrite.js, variable.other.object.js, variable.parameter.function.js, keyword.operator.assignment.js, punctuation.separator.key-value.js, string.unquoted.label.js
  settings: 
    foreground: '#f1f1f1'

- name: Javascript - Yellow
  scope: keyword.control.flow.js, variable.language.this.js, keyword.control.conditional.js
  settings: 
    foreground: '#EDE480'

- name: Javascript - Light Blue
  scope: string.quoted.single.js, string.quoted.single.js entity.name.function.js, variable.parameter.function.js
  settings: 
    foreground: '#BDDCDC'

- name: Javascript - Blue
  scope: meta.group.braces.curly meta.property.class.js variable.other.class.js, source.js meta.group.braces.round meta.function-call.with-arguments.js variable.function.js
  settings: 
    foreground: '#92BFBF'

- name: Javascript - Green
  scope: constant.language.boolean.true.js, constant.language.boolean.false.js
  settings: 
    foreground: '#B4D388'

- name: Javascript - Orange
  scope: meta.group.braces.curly meta.property.class.js variable.other.property.static.js, constant.language.boolean.false.js, source.js meta.group.braces.round meta.function-call.method.with-arguments.js variable.function.js
  settings: 
    foreground: '#F49D62'

- name: Handlebars Text
  scope: text.html.handlebars
  settings: 
    foreground: '#f1f1f1'

- name: Handlebars Text
  scope: text.html.handlebars meta.function.inline.other.handlebars variable.parameter.handlebars, text.html.django storage.type.variable.django string.unquoted.tag-string.django
  settings: 
    foreground: '#F49D62'

- name: Handlebars Braces
  scope: text.html.handlebars meta.function.inline.other.handlebars support.constant.handlebars, meta.function.block.start.swig, support.constant.swig, support.constant.handlebars, entity.tag.tagbraces.django
  settings: 
    foreground: '#f1f1f1'

- name: Handlebars Loops
  scope: text.html.handlebars meta.function.block.start.handlebars support.constant.handlebars, string.unquoted.tag-string.django, keyword.control.filter.django
  settings: 
    foreground: '#92BFBF'

- name: Keyword
  scope: keyword
  settings: 
    foreground: '#F49D62'

- name: Storage
  scope: storage
  settings: 
    foreground: '#EDE480'

- name: String
  scope: string
  settings: 
    foreground: '#BDDCDC'

- name: Support
  scope: support
  settings: 
    foreground: '#92BFBF'

- name: Variable
  scope: variable
  settings: 
    foreground: '#F49D62'

- name: Red
  scope: invalid, keyword.other.important.scss
  settings: 
    foreground: '#CF5340'

- name: Embedded Source (Bright)
  scope: text.html.ruby source
  settings: 
    foreground: '#151515'

- name: Entity inherited-class
  scope: entity.other.inherited-class
  settings: 
    foreground: '#B4D388'

- name: String embedded-source
  scope: string.quoted source
  settings: 
    foreground: '#B4D388'

- name: String constant
  scope: string constant
  settings: 
    foreground: '#B4D388'

- name: Regexp
  scope: string.regexp
  settings: 
    foreground: '#F49D62'

- name: String variable
  scope: string variable
  settings: 
    foreground: '#EDE480'

- name: Support function
  scope: support.function
  settings: 
    foreground: '#F49D62'

- name: Support constant
  scope: support.constant
  settings: 
    foreground: '#B4D388'

- name: C and C++ Preprocessor Line
  scope: other.preprocessor.c
  settings: 
    foreground: '#92BFBF'

- name: C and C++ Preprocessor Directive
  scope: other.preprocessor.c entity
  settings: 
    foreground: '#92BFBF'

- name: Meta.tag.A
  scope: declaration.tag, declaration.tag entity, meta.tag, meta.tag entity
  settings: 
    foreground: '#B4D388'

- name: css tag-name
  scope: meta.selector.css entity.name.tag, entity.name.tag.scss, source.scss entity.other.attribute-name.pseudo-class.scss, keyword.control.at-rule.include.scss, entity.name.tag.wildcard.scss
  settings: 
    foreground: '#EDE480'

- name: css id
  scope: meta.selector.css entity.other.attribute-name.id, meta.selector.scss entity.other.attribute-name.id, entity.name.tag.reference.scss
  settings: 
    foreground: '#F49D62'

- name: css class
  scope: meta.selector.css entity.other.attribute-name.class, source.scss entity.other.attribute-name.class.css, keyword.other.default.scss
  settings: 
    foreground: '#B4D388'

- name: css property-name psuedos and attibutes
  scope: support.type.property-name.css, invalid.illegal, support.type.property-name.scss, entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.pseudo-class.css, entity.other.attribute-name.attribute.css, variable.scss, meta.property-name.scss, source.css meta.property-list.css meta.property-name.css
  settings: 
    foreground: '#92BFBF'

- name: css property-value
  scope: meta.property-group support.constant.property-value.css, meta.property-value support.constant.property-value.css, meta.property-group support.constant.property-value.scss, meta.property-value support.constant.property-value.scss, meta.property-list meta.property-value.scss
  settings: 
    foreground: '#EDE480'

- name: css at-rule
