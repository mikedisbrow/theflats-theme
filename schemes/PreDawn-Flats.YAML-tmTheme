  settings: 
    foreground: '#F49D62'

- name: html class
  scope: entity.other.attribute-name.html
  settings: 
    foreground: '#EDE480'

- name: html punctuations tags
  scope: punctuation.definition.tag.end, punctuation.definition.tag.begin, punctuation.definition.tag
  settings: 
    foreground: '#92BFBF'

- name: scss terminators
  scope: punctuation.section.function.scss, punctuation.terminator.rule.scss, punctuation.separator.key-value.scss
  settings: 
    foreground: '#F1F1F1'

- name: css at rules
