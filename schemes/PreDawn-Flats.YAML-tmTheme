- name: PHP Keyword
  scope: keyword.control.php
  settings: 
    foreground: '#B4D388'

- name: PHP Keyword Operator
  scope: keyword.operator.logical.php, keyword.operator.assignment.php, keyword.operator.class.php
  settings: 
    foreground: '#999999'

- name: PHP Variable
  scope: variable.other.property.php
  settings: 
    foreground: '#EDE480'

- name: PHP Strings
  scope: string.regexp.single-quoted.php
  settings: 
    foreground: '#BDDCDC'

# HIGHLIGHTER

- name: Bracket Default
  scope: brackethighlighter.default
  settings: 
    foreground: '#92BFBF'

- name: WordHighlight
  scope: wordhighlight
  settings:
    foreground: '#f49d62'
